:: TraditionalReading
<<py
# Add experience
reader.add_experience(10)

# Modify trust based on style match
if reader.style == "traditional":
    client.modify_trust(5)
else:
    client.modify_trust(-2)

# Record cards
for card in cards:
    client.add_card_seen(card)

interpretation = f"The {cards[1].name} in your present suggests significant changes."
>>

